{
    "name": "@rachelallyson/planning-center-monorepo",
    "version": "1.0.0",
    "private": true,
    "description": "Planning Center Online API client libraries - monorepo",
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "build": "npm run build --workspaces",
        "build:base": "npm run build --workspace=@rachelallyson/planning-center-base-ts",
        "build:people": "npm run build --workspace=@rachelallyson/planning-center-people-ts",
        "clean": "npm run clean --workspaces",
        "test": "npm run test --workspaces",
        "test:base": "npm run test --workspace=@rachelallyson/planning-center-base-ts",
        "test:people": "npm run test --workspace=@rachelallyson/planning-center-people-ts",
        "lint": "echo 'Linting not configured'",
        "prepare": "npm run build:base",
        "dev:docs": "cd docs && next dev",
        "build:docs": "cd docs && next build",
        "start:docs": "cd docs && next start"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/react": "^19.0.0",
        "next": "^15.1.3",
        "nextra": "^4.6.0",
        "nextra-theme-docs": "^4.6.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "typescript": "^5.9.3"
    },
    "engines": {
        "node": ">=20.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/rachelallyson/planning-center-monorepo.git"
    },
    "author": "Rachel Allyson",
    "license": "MIT"
}